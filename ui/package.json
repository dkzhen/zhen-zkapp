{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts-watch": "tsc --noEmit --incremental --watch",
    "build:css": "postcss styles/globals.css -o styles/main.css",
    "build:watch": "postcss styles/globals.css -o styles/main.css --watch",
    "prod:build": "NODE_ENV=production yarn run build:css && yarn run build",
    "predeploy": "npm run build",
    "deploy": "build:css && gh-pages -d deploy"
  },
  "dependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "postcss-cli": "^10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "snarkyjs": "0.*",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
